/* Smartphones (portrait and landscape) ———– */
@media only screen and (min-device-width: 320px) and (max-device-width: 480px) {
  /* @media only screen */
  /* and (max-width : 1024px) { */
  /* Styles */
  #leftcolumn {
    position: relative;
    width: 100%;
    height: auto;
    padding: 0;
  }
  #leftcolumn > nav a {
    width: 100%;
  }
  #leftcolumn > nav a > img {
    width: 100%;
  }
  #leftcolumn nav.desktop {
    display: none;
  }
  #leftcolumn nav.mobile {
    display: block;
    margin-top: 0;
    width: 100%;
    text-align: center;
    margin: 0 auto;
    position: relative;
    top: 0;
  }
  #leftcolumn nav.mobile #logo {
    position: absolute;
    display: block;
  }
  #leftcolumn nav.mobile #logo img {
    display: block;
    width: 100px;
  }
  #leftcolumn nav.mobile form {
    position: relative;
    padding: 10px 20px 0 120px;
  }
  #leftcolumn nav.mobile form select {
    width: 100%;
  }
  #leftcolumn nav.mobile .goButton {
    width: auto;
    padding: 2px 10px;
    display: inline;
    text-transform: inherit;
  }
  #rightcolumn {
    position: relative;
    height: auto;
  }
  #rightcolumn input[type=text] {
    width: 200px;
  }
  #rightcolumn #content-wrapper {
