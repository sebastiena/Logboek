/********************************
TYPO
*********************************/
body {
  font-size: 12px;
  line-height: 18px;
  font-family: Arial, Helvetica, sans-serif;
  color: #666666;
}
h1, h2, h3 {
  font-family: 'Swis721LtCnBTLight', Helvetica, Arial, sans-serif;
  font-weight: normal;
  font-style: normal;
  padding: 0;
  margin: 0;
  margin-bottom: 20px;
}
ul, ol {
  margin: 0;
  padding: 0 15px;
  margin-bottom: 20px;
}
ol {
  margin-bottom: 0;
  padding-bottom: 10px;
}
ol li {
  color: #ca0928;
  font-weight: bold;
}
p {
  margin-top: 0px;
  margin-bottom: 20px;
}
p.small {
  font-size: 9px;
}
h1 {
  font-size: 24px;
  line-height: 30px;
  color: #333333;
  margin-bottom: 30px;
}
h2 {
  font-family: 'Swis721LtBTLight', Helvetica, Arial, sans-serif;
  font-size: 18px;
  color: #ca0928;
  line-height: 30px;
  -webkit-column-break-after: never;
  -moz-column-break-after: never;
  column-break-after: never;
}
h3 {
  font-weight: bold;
}
a {
  color: #797e9b;
  text-decoration: underline;
}
a:hover, a:hover:visited {
  color: #ca0928;
}
a:visited {
  color: #797e9b;
}
/********************************
ELEMENTS
*********************************/
html, body {
  height: 100%;
  width: 100%;
  background: #ebebeb;
}
#language-choice {
  margin: 0 auto;
  width: 250px;
  background-color: #fff;
  position: relative;
  top: 100px;
  text-align: center;
  padding: 10px 20px 20px 20px;
  border: 1px solid #dedede;
}
#language-choice a {
  font-size: 14px;
}
#leftcolumn {
  padding: 0px 50px;
  width: 230px;
  border-right: 1px solid #bebebe;
  height: 100%;
  position: fixed;
  background: white;
  z-index: 2;
}
#leftcolumn > nav {
  position: absolute;
  top: 35%;
  margin-top: -172px;
}
#leftcolumn > nav ul li a {
  height: 40px;
  display: block;
  border-bottom: 1px solid #ebebeb;
  color: #666666;
  text-decoration: none;
  line-height: 40px;
  font-family: 'Swis721LtBTLight', Helvetica, Arial, sans-serif;
  font-size: 14px;
  text-transform: uppercase;
  width: 100%;
}
#leftcolumn > nav ul li a:hover, #leftcolumn > nav ul li a.active {
  color: #ca0928;
}
#leftcolumn nav.mobile {
  display: none;
}
#rightcolumn {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  /* fix clearfix: blueprintcss.lighthouseapp.com/projects/15318/tickets/5-extra-margin-padding-bottom-of-page */

  zoom: 1;
}
#rightcolumn:before, #rightcolumn:after {
  content: "\0020";
  display: block;
  height: 0;
  visibility: hidden;
}
#rightcolumn:after {
  clear: both;
}
#rightcolumn #content-wrapper {
  padding-left: 330px;
  height: auto;
  /* fix clearfix: blueprintcss.lighthouseapp.com/projects/15318/tickets/5-extra-margin-padding-bottom-of-page */

  zoom: 1;
}
#rightcolumn #content-wrapper:before, #rightcolumn #content-wrapper:after {
  content: "\0020";
  display: block;
  height: 0;
  visibility: hidden;
}
#rightcolumn #content-wrapper:after {
  clear: both;
}
#rightcolumn #content-wrapper #content {
  float: left;
  position: relative;
  width: 100%;
}
#rightcolumn #content-wrapper #content header {
  background: url(../img/headerbg.jpg) no-repeat;
  background-size: cover;
  height: 268px;
  border-bottom: 1px solid #bebebe;
  position: relative;
}
#rightcolumn #content-wrapper #content header h1 {
  color: #ca0928;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 14px;
  font-weight: bold;
  display: inline-block;
  padding: 0px 10px;
  position: absolute;
  bottom: 0px;
  left: 0;
  margin: 0;
}
#rightcolumn #content-wrapper #content section {
  padding: 50px 50px 30px 50px;
  margin-bottom: 20px;
}
#rightcolumn #content-wrapper #content section h1 {
  border-left: #ca0928 4px solid;
  padding-left: 15px;
}
#rightcolumn #content-wrapper aside {
  background: #ca0928;
  color: white;
  float: left;
}
#rightcolumn #content-wrapper aside section {
  padding: 50px 50px 30px 50px;
  margin-bottom: 20px;
}
#rightcolumn #content-wrapper aside h1 {
  border-left: white 4px solid;
  color: white;
  padding-left: 15px;
}
#rightcolumn #content-wrapper aside h2 {
  color: white;
}
#rightcolumn #content-wrapper aside a {
  color: #ec9b00;
}
#rightcolumn #content-wrapper aside img {
  float: right;
}
footer {
  width: 250px;
  position: absolute;
  bottom: 10px;
  /* fix clearfix: blueprintcss.lighthouseapp.com/projects/15318/tickets/5-extra-margin-padding-bottom-of-page */

  zoom: 1;
}
footer:before, footer:after {
  content: "\0020";
  display: block;
  height: 0;
  visibility: hidden;
}
footer:after {
  clear: both;
}
footer nav {
  font-size: 8px;
  margin: 0 7px 0 0;
}
footer nav:last-child {
  margin: 0;
}
footer nav a {
  font-size: 9px;
  color: #666666;
}
footer nav a:hover {
  color: #ca0928;
}
footer #language-switch {
  display: inline;
  font-size: 8px;
}
footer.mobile {
  display: none;
}
#personal-code #loader {
  display: none;
}
#personal-code #loader img {
  float: none;
}
#personal-code #error {
  display: none;
  color: white;
  background-color: red;
  border: 1px solid #b30000;
  padding: 5px 8px;
}
#personal-code #thanks {
  display: none;
  font-style: italic;
}
#personal-code #thanks.show {
  display: block;
}
/******************
STYLING
******************/
.cta {
  -webkit-column-break-after: never;
  -moz-column-break-after: never;
  column-break-after: never;
  background: #ec9b00;
  display: block;
  color: white;
  font-size: 14px;
  padding: 20px;
  text-align: center;
  text-decoration: none;
  margin-bottom: 10px;
  cursor: pointer;
  border: none;
  text-transform: uppercase;
  max-width: 350px;
}
.cta:hover {
  background: #fdac11;
  color: white !important;
}
.cta:active, .cta:visited {
  color: white;
}
.btncode {
  margin-top: 30px;
  background: url(../img/button_speldoos.jpg) no-repeat;
  padding: 25px 0 0 105px;
  width: 129px;
  height: 55px;
  display: block;
  color: white;
  text-transform: uppercase;
  text-decoration: none;
  cursor: pointer;
}
.btncode:visited, .btncode:hover, .btncode:visited:hover {
  color: white;
}
input[type=text] {
  border: 0;
  background: white;
  width: 320px;
  height: 35px;
  padding: 0 5px;
  margin-bottom: 15px;
}
input[type=submit] {
  -webkit-column-break-after: never;
  -moz-column-break-after: never;
  column-break-after: never;
  background: #ec9b00;
  display: block;
  color: white;
  font-size: 14px;
  padding: 20px;
  text-align: center;
  text-decoration: none;
  margin-bottom: 10px;
  cursor: pointer;
  border: none;
  text-transform: uppercase;
  max-width: 350px;
  padding: 10px 20px;
}
input[type=submit]:hover {
  background: #fdac11;
  color: white !important;
}
input[type=submit]:active, input[type=submit]:visited {
  color: white;
}
input[type=submit].disabled {
  background-color: grey;
  color: #333;
  cursor: wait;
}
.col {
  display: block;
  -webkit-column-fill: balanced;
  -moz-column-fill: balanced;
  column-fill: balanced;
}
.col img {
  width: 100%;
  max-width: 390px;
}
.col p {
  text-align: justify;
  -webkit-column-break-inside: avoid;
  -moz-column-break-inside: avoid;
  column-break-inside: avoid;
}
.col .cta {
  -webkit-column-break-inside: avoid;
  -moz-column-break-inside: avoid;
  column-break-inside: avoid;
}
.first.column p {
  padding-right: 20px;
}
.nobreakcol {
  -webkit-column-break-after: never;
  -moz-column-break-after: never;
  column-break-after: never;
}
/*accordion voor vragen pagina*/
details summary::-webkit-details-marker {
  display: none;
}
details {
  display: block;
  background: #ca0928;
  color: white;
  padding: 10px 20px 10px 0;
  border-bottom: 1px solid #ebebeb;
}
details > * {
  display: none;
}
details:last-of-type {
  margin-bottom: 20px;
}
details summary {
  display: block;
  font-size: 18px;
  background: url(../img/arrow.jpg) no-repeat;
  background-position: 15px 4px;
  padding-left: 30px;
  font-family: 'Swis721LtCnBTLight', Helvetica, Arial, sans-serif;
  font-weight: normal;
  font-style: normal;
  display: block;
  text-decoration: none;
}
details div {
  visibility: hidden;
  padding: 20px 20px 20px 30px;
}
details.open summary, details[open=open] summary {
  margin-bottom: 10px;
  background: url(../img/arrow_down.jpg) no-repeat;
  background-position: 15px 4px;
  outline: none;
}
details.open summary:active, details[open=open] summary:active {
  outline: none;
}
details.open a, details[open=open] a {
  color: #ec9b00;
  font-weight: bold;
}
details.open a:hover,
details[open=open] a:hover,
details.open a:active,
details[open=open] a:active,
details.open a:visited,
details[open=open] a:visited {
  color: #ec9b00;
}
details.open div, details[open=open] div {
  visibility: visible;
}
